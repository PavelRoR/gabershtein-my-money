.revs-slider.slick-dotted.slick-slider
.revs-slider-text.slick-dotted.slick-slider
	width 100%
	max-width 1920px
	margin 0 auto 180px
	position relative
	z-index 50
	@media screen and (max-width 560px)
		margin-bottom: 100px
	.slick-track
		padding-bottom: 20px
	.slick-center
		transform scale(1.08)
	.slick-arrow
		height 0
		width 0
		top auto
		bottom: -90px
	.slick-prev
	.slick-next
		&:before
			width 100%
			height 100%
			border-radius: 50px
			display block
			line-height: 46px
			background-color: #769b72
			text-align center
			color #fff
			font-size: 24px
			opacity 1
			transition: all .3s ease
			border: 1px solid #769b72;
			font-weight: normal
		&:hover
			&:before
				background-color: transparent
				color #769b72
		left 43%
	.slick-next
		left auto
		right 43%
.revs-slider-text.slick-dotted.slick-slider
	margin-bottom: 100px
	@media screen and (max-width 991px)
		margin-bottom: 60px
.slick-dots
	left 0
	text-align center
	bottom: -30px
	right 0
	margin 0 auto
	li
		width 10px
		height 10px
		border-radius: 50px
		border: 2px solid #769b72;;
		transition: all .3s ease
		margin 0 2px
		overflow hidden
		button
			transition: all .3s ease
			width 100%
			height 100%
			background-color: transparent
			padding 0
			&:before
				content: none
		&.slick-active
			button
				background-color: #769b72;
.text-rev-img
	position relative
	// z-index 30
	width 100%
	max-width 560px
	display block
	margin 0 auto
	background: #BCE2E6;
	object-fit: contain
	object-position: center
	height 350px
	border-radius: 5px
	@media screen and (max-width 991px)
		height 200px
	@media screen and (max-width 768px)
		height 300px
	@media screen and (max-width 386px)
		height: 200px
.text-rev-link
	padding 20px
.video-revs-arrows-1
.video-revs-arrows-2
	position absolute
	width 110px
	top 100px
	left 0
	right 0
	margin 0 auto
	z-index 100
	@media screen and (max-width 560px)
		top 90px
	@media screen and (max-width 386px)
		top 80px
.slick-next, .slick-prev
	width 60px
	height 60px
.slick-next:before
.slick-prev:before
	color: #769b72
	font-size: 60px
	font-weight: normal
	transition: all .3s ease
	opacity 1
.slick-next:hover:before
.slick-prev:hover:before
	color: #769b72 + 20%
.yt-button
	position absolute
	cursor pointer
	left: calc(50% - 34px);
	top: calc(50% - 24px);
	&:before
		content: ''
		display block
		position: absolute;
		// left: calc(50% - 34px);
		// top: calc(50% - 124px);
		width: 68px;
		height: 48px;
		border-radius 7px
		background-color: rgba(0,0,0,.7)
		transition: all .15s ease
		z-index 68
		@media screen and (max-width 560px)
			background-color: #FF0000
	&:hover
		&:before
			background-color: #FF0000
	&:after
		content: ''
		display block
		position absolute
		border: 10px solid transparent;
		border-left: 18px solid #fff
		width 10px
		height 10px
		// left: calc(50% - 8px);
		// top: calc(50% - 110px);
		left: 27px
		top: 15px
		z-index 70
.slick-slide
.slick-center
	transition: all .3s ease
.general-img-rev-1
	right -50px
	top -130px
#last
	padding 0 0 140px
	background-image: url(../img/backs/clouds.png)
	background-repeat: no-repeat
	background-position: center bottom
	background-size 100% 100%
	@media screen and (min-width 1900px)
		background-image: none
	@media screen and (max-width 991px)
		padding 0 0 100px
		background-size contain
		background-position: center
	// @media screen and (max-width 560px)
.last-title
	text-align center
	font-size: 54px
	font-family: ADP
	font-style: italic
	margin-bottom: 40px
	@media screen and (max-width 991px)
		font-size: 38px
	@media screen and (max-width 560px)
		font-size: 32px
.form
	&-last
		padding 15px 160px
		overflow hidden
		@media screen and (max-width 1199px)
			padding 15px 60px
		@media screen and (max-width 560px)
			padding 15px 30px
		@media screen and (max-width 386px)
			padding 15px
.general-title-revs
	@media screen and (max-width 560px)
		margin-bottom: 80px