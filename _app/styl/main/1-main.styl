.input
	display inline-block
	width 100%
	max-width 374px
	border: 1px solid #7fa37d;
	border-radius: 10px;
	font-family: C
	font-size: 16px
	font-weight: 300
	color #8a8888
	padding 32px 22px
	background-color: #FFF
	&-mail-main
		float: left
		@media screen and (max-width 991px)
			float: none
			display block
			margin: 0 auto 20px
	&-mail-modal
		float: none
		display block
		margin 0 auto 20px
	&::-webkit-input-placeholder
		color:#8a8888
		opacity:1;
	&::-moz-placeholder
		color:#8a8888
		opacity:1;
	&:-ms-input-placeholder
		color:#8a8888
		opacity:1;
	@media screen and (max-width 991px)
		padding 18px 30px
@-webkit-keyframes autofill
	to
		font-size: 16px
		color: #8a8888;
		background-color: #FFF
input:-webkit-autofill
	-webkit-animation-name: autofill;
	-webkit-animation-fill-mode: both;
.input:active
input:-internal-autofill-selected
	background-color: #FFF !important
.button-form
	float: right
	padding 30px 22px
	width 100%
	max-width 374px
	box-shadow: inset 0px 17px 20.16px 0.84px rgba(255, 255, 255, 0.7)
	@media screen and (max-width 991px)
		display block
		margin 0 auto
		float: none
	@media screen and (max-width 386px)
		padding 20px 15px
	&-modal
		float: none
		display block
		margin 0 auto
.alert-message
	clear both
	display: none;
	font-size: 16px;
	padding-top: 10px;
	color: #f00
	text-align: center
	font-weight: 700
	line-height: 1
.checking
	display: table;
	width auto
	// max-width 455px
	margin: 5px 0 0
	text-align: left
	@media screen and (max-width 991px)
		margin 5px auto 0
	@media screen and (max-width 767px)
		max-width 380px
@keyframes pulse
	0%
		box-shadow: 0 0 0 0 rgba(red, .12)
	100%
		box-shadow: 0 0 15px 10px rgba(red, .12)
.checking .check-label
	line-height: 28px;
	cursor: pointer;
	position: relative;
	display: table-cell;
	vertical-align: middle;
	padding: 0 0 0 25px;
	text-align: left;
	font-family: C
	// font-size: 13px
	font-size: 18px
	font-weight: 300
	// color #b5b3b3
	color #000
	@media screen and (max-width 767px)
		line-height: 18px
.checking .check-label a
	text-decoration: none
	border-bottom: 1px dashed transparent
	display: inline
	color #b5b3b3
	&:focus
		border-bottom-color: transparent
		color #b5b3b3
	&:hover
		border-bottom-color: inherit
		text-decoration: none
		color #b5b3b3
.checking .check-label:before
	content: '';
	background-color: red;
	position: absolute;
	left: 0;
	top: 3px;
	border-radius: 5px;
	width: 18px;
	height: 18px;
	animation: pulse
	animation-duration: .7s
	animation-iteration-count: infinite
	animation-timing-function: ease-out;
.checking .check-label:after
	content: '';
	position: absolute;
	left: 6px;
	top: -10px;
	width: 6px;
	height: 10px;
	background-color: transparent;
	opacity: 0;
	border-bottom: 2px solid #fff;
	border-right: 2px solid #fff;
	-webkit-transform: rotate(41deg);
	-ms-transform: rotate(41deg);
	transform: rotate(41deg);
	-webkit-transition: all 0.5s ease;
	-o-transition: all 0.5s ease;
	transition: all 0.5s ease;
.check
	position: absolute;
	z-index: -1;
	opacity: 0;
	width: 0;
	height 0;
	border: 0;
	left -99999px;
.check:checked + .check-label:before
	// background-color: rgb(173,200,179)
	background-color: #000
	// background-image: linear-gradient(to top, rgb(173,200,179) 0%, rgb(118,155,114) 100%);
	// box-shadow: inset 0px 17px 20.16px 0.84px rgba(255, 255, 255, 0.89);
	animation: none
.check:checked + .check-label:after
	opacity: 1;
	top: 6px;
	-webkit-transition: all 0.5s ease;
	-o-transition: all 0.5s ease;
	transition: all 0.5s ease;