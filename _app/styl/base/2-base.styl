*
	margin 0
	padding 0
	outline none
body
	font-size: 18px
	font-family: C
	color #313030
	background-color #fff
	line-height: 1.56
.wrapper
	overflow hidden
.content
	position relative
	z-index 90
a
	color: #000
	&:focus
		outline: 0
		text-decoration none
	&:hover
		text-decoration none
h1,h2,h3,h4,h5,h6
	font-weight normal
	margin 0
button
	border none
section
header
footer
	width 100%
	display block
.video-wrapper
	margin 0 auto
	position relative
	width 100%
	top 0
	// z-index 60
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #000
		border-radius 0
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
		margin 0 auto
.button
	&-wrapper
		position relative
		width 100%
		max-width 374px
		margin 0 auto
		z-index 99
		&:before
			content: ''
			display block
			position absolute
			width 90%
			bottom: 0
			left 0
			right 0
			margin 0 auto
			height: 10px
			box-shadow: 0px 10px 12px 0px rgba(0, 0, 0, 0.35);
			z-index -1
			transition: all .4s ease
		&:hover:before
			box-shadow: 0px 15px 12px 0px rgba(0, 0, 0, 0.42);
			width 96%
	display block
	text-align center
	margin 0 auto
	text-decoration none
	color: #fff;
	width: 100%
	font-family: C
	font-size: 20px
	font-weight: bold
	text-transform uppercase
	z-index 50
	border-radius: 10px;
	background-image: rgb(173,200,179)
	background-image: linear-gradient(to top, rgb(118,155,114) 0%, rgb(173,200,179) 100%);
	box-shadow: inset 0px 17px 20.16px 0.84px rgba(255, 255, 255, 0.7), 0 5px 0 0 #527a5a
	padding: 50px 25px
	transition: all .4s ease
	@media screen and (max-width 560px)
		padding 25px
	@media screen and (max-width 386px)
		padding 20px 15px
		font-size: 18px
	&:focus
		text-decoration none
		color #fff
	&:hover
		text-decoration none
		color #fff
		box-shadow: inset 0px 17px 20.16px 0.84px rgba(255, 255, 255, 0.45), 0 0 0 0 #527a5a
		transform: translateY(5px)
		.button-wrapper:before
			box-shadow: 0px 15px 12px 0px rgba(0, 0, 0, 0.55);
			width 96%
.general-title
	text-align center
	font-family: ADP
	font-size: 45px
	font-style: italic
	margin-bottom: 80px
	line-height: 53px
	&-green
		color #769b72
	@media screen and (max-width 560px)
		font-size: 32px
		margin-bottom: 40px
		line-height: 38px
	@media screen and (max-width 386px)
		font-size: 26px
		margin-bottom: 30px
		line-height: 30px
.general-list
	li
		list-style: none
		position relative
		margin-bottom: 35px
		padding-left: 40px
		@media screen and (max-width 560px)
			margin-bottom: 20px
		&:last-child
			margin-bottom: 0
		&:before
			content: ''
			display block
			position absolute
			top -1px
			left 0
			border-radius: 50%;
			background-image: linear-gradient(to bottom, rgb(221,193,107) 0%, rgb(241,224,172) 100%);
			box-shadow: inset 0px 50px 41.16px 7.84px rgba(255, 255, 255, 0.5);
			width: 30px;
			height: 30px;
		&:after
			content: ''
			position absolute
			display block
			width 9px
			height: 12px
			border-bottom: 3px solid #000
			border-right: 3px solid #000
			transform: rotate(35deg)
			top 6px
			left 10px
.general-img
	position absolute
	z-index -1
.mb-none
	@media screen and (max-width 991px)
		display none